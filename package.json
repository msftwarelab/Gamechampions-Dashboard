{
  "name": "game-champions",
  "description": "Game Champions",
  "version": "1.0.482",
  "main": "./app.js",
  "author": {
    "name": "Incredible Web",
    "email": "info@incredible-web.com",
    "url": "https://www.incredible-web.com"
  },
  "scripts": {
    "deploy": "cross-env DEPLOY=true npm run build",
    "deploy:dev": "cross-env DEPLOY=true npm run build:dev",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "build:analyze": "cross-env NODE_ENV=production webpack --config-name web --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development",
    "dev": "concurrently --names BUILD,SERVER npm:watch npm:start_watch",
    "watch": "cross-env NODE_ENV=development webpack --watch --mode development",
    "start": "node app.js",
    "start_watch": "wait-on app.js dist/inline.css && nodemon --inspect app.js --config nodemon.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
